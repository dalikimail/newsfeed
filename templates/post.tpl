<div class="row">
	<div class="col" id="post{{POST-ID}}">
		<header>
			<span class="font-weight-bold">{{POST-AUTHOR}}</span>
			<span class="float-right post-date">{{POST-DATE}}</span>
			{{POST-MANAGER}}
		</header>
		<h5>{{POST-TITLE}}</h5>
		<p>{{POST-CONTENT}}</p>
		<p>{{TAG-LIST}}</p>
	</div>
</div>
